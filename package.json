{
  "name": "@macrostrat/ui-components",
  "version": "0.2.0",
  "description": "UI components for React and blueprint.js",
  "repository": "https://github.com/UW-Macrostrat/ui-components.git",
  "style": "lib/index.css",
  "module": "lib/esm",
  "main": "lib/cjs",
  "typings": "lib/types",
  "scripts": {
    "watch:main": "rollup -c -w",
    "watch:types": "tsc --emitDeclarationOnly -w",
    "watch:docs": "rm -rf .docz && docz dev",
    "dev": "concurrently npm:watch:*",
    "build:docs": "rm -rf .docz && docz build",
    "build:main": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "concurrently npm:build:*",
    "prepare": "npm run build"
  },
  "author": "Daven Quinn",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@blueprintjs/datetime": "^3.7.0",
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "@macrostrat/hyper": "^1.2.6",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "core-js": "^3.6.4",
    "date-fns": "^1.30.1",
    "immutability-helper": "^3.0.1",
    "prop-types": "^15.7.2",
    "react-dropzone": "^8.2.0",
    "react-hyperscript": "^3.2.0",
    "react-infinite-scroller": "^1.2.4",
    "react-json-view": "^1.19.1",
    "underscore": "^1.9.1",
    "use-async-effect": "^2.2.1",
    "@blueprintjs/core": "^3.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.8.3",
    "@betit/rollup-plugin-rename-extensions": "^0.1.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@types/classnames": "^2.2.9",
