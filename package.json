{
  "name": "@macrostrat/ui-components",
  "version": "0.3.0",
  "description": "UI components for React and blueprint.js",
  "repository": "https://github.com/UW-Macrostrat/ui-components.git",
  "style": "lib/index.css",
  "module": "lib",
  "types": "lib/types",
  "scripts": {
    "watch:main": "rollup -c -w",
    "watch:docs": "rm -rf .docz && docz dev",
    "dev": "npm run watch:main",
    "dev:docs": "npm-run-all -p --aggregate-output npm:watch:**",
    "build:docs": "rm -rf .docz && docz build",
    "build": "rollup -c",
    "prepare": "npm run build"
  },
  "author": "Daven Quinn",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@blueprintjs/core": "^3.17.1",
    "@blueprintjs/datetime": "^3.7.0",
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "@macrostrat/hyper": "^1.2.10",
    "@types/classnames": "^2.2.9",
    "@types/immutability-helper": "^2.6.3",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.8",
    "@types/react-hyperscript": "^3.0.4",
    "@types/react-router-dom": "^5.1.5",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "core-js": "^3.6.4",
    "date-fns": "^1.30.1",
    "fp-ts": "^2.5.3",
    "immutability-helper": "^3.0.1",
    "io-ts": "^2.1.2",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.1",
    "react-dropzone": "^8.2.0",
    "react-hyperscript": "^3.2.0",
    "react-infinite-scroller": "^1.2.4",
    "react-json-view": "^1.19.1",
    "react-virtualized": "^9.21.2",
    "react-window": "^1.8.5",
    "underscore": "^1.9.1",
    "use-async-effect": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.8.3",
    "@rollup/plugin-typescript": "^5.0.2",
    "babel-plugin-emotion": "^10.0.14",
    "docz": "^2.2.0",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "postcss-modules": "^2.0.0",
    "pretty-quick": "^2.0.1",
    "stylus": "^0.54.7",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
